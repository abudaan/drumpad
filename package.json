    "babel-preset-stage-0": "^6.24.1",
    "json5": "^2.1.0",
    "parcel": "^1.11.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.15.2",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "copy": "mkdir -p dist/data && cp data/* dist/data",
    "clean": "rm -rf .cache && rm -rf dist",
    "start": "parcel index.html",
    "start-secure": "parcel index.html --https",
    "all": "npm run clean && npm run copy && npm run start",
    "build": "parcel build index.html --public-url='/drummachine'"
